{
  "name": "@coscientist/monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@coscientist/web",
    "dev:hq": "turbo dev --filter=@coscientist/hq",
    "build": "turbo build",
    "build:web": "turbo build --filter=@coscientist/web",
    "build:hq": "turbo build --filter=@coscientist/hq",
    "check": "bun x ultracite@latest check",
    "lint": "bun x ultracite@latest fix",
    "format": "bun x ultracite@latest fix",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules .turbo"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "bun@1.2.19",
  "engines": {
    "node": ">=24 <25"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.36.0"
  }
}
