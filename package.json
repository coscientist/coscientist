{
  "name": "coscientist-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@repo/web",
    "dev:hq": "turbo dev --filter=@repo/hq",
    "build": "turbo build",
    "build:web": "turbo build --filter=@repo/web",
    "build:hq": "turbo build --filter=@repo/hq",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules .turbo"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "bun@1.2.19",
  "engines": {
    "node": ">=18.0.0"
  }
}
